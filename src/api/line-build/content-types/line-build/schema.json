{
  "kind": "collectionType",
  "collectionName": "line_builds",
  "info": {
    "singularName": "line-build",
    "pluralName": "line-builds",
    "displayName": "Line Build",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "Description": {
      "type": "text"
    },
    "DropPhrase": {
      "type": "string"
    },
    "BuildSpecs": {
      "type": "richtext"
    },
    "Media": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Components": {
      "type": "component",
      "repeatable": true,
      "component": "ingredients.ingredient-list"
    },
    "Allergen": {
      "type": "component",
      "repeatable": true,
      "component": "allergens.allergens"
    },
    "Locations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::location.location",
      "mappedBy": "line_builds"
    },
    "Great": {
      "type": "text"
    },
    "Watch": {
      "type": "text"
    }
  }
}
